def RequestException(IOError):
    """
    There was an ambiguous exception that occurred while handling your
        request.
    
    """
    def __init__(self, *args, **kwargs):
        """
        Initialize RequestException with `request` and `response` objects.
        """
def HTTPError(RequestException):
    """
    An HTTP error occurred.
    """
def ConnectionError(RequestException):
    """
    A Connection error occurred.
    """
def ProxyError(ConnectionError):
    """
    A proxy error occurred.
    """
def SSLError(ConnectionError):
    """
    An SSL error occurred.
    """
def Timeout(RequestException):
    """
    The request timed out.

        Catching this error will catch both
        :exc:`~requests.exceptions.ConnectTimeout` and
        :exc:`~requests.exceptions.ReadTimeout` errors.
    
    """
def ConnectTimeout(ConnectionError, Timeout):
    """
    The request timed out while trying to connect to the remote server.

        Requests that produced this error are safe to retry.
    
    """
def ReadTimeout(Timeout):
    """
    The server did not send any data in the allotted amount of time.
    """
def URLRequired(RequestException):
    """
    A valid URL is required to make a request.
    """
def TooManyRedirects(RequestException):
    """
    Too many redirects.
    """
def MissingSchema(RequestException, ValueError):
    """
    The URL schema (e.g. http or https) is missing.
    """
def InvalidSchema(RequestException, ValueError):
    """
    See defaults.py for valid schemas.
    """
def InvalidURL(RequestException, ValueError):
    """
    The URL provided was somehow invalid.
    """
def InvalidHeader(RequestException, ValueError):
    """
    The header value provided was somehow invalid.
    """
def InvalidProxyURL(InvalidURL):
    """
    The proxy URL provided is invalid.
    """
def ChunkedEncodingError(RequestException):
    """
    The server declared chunked encoding but sent an invalid chunk.
    """
def ContentDecodingError(RequestException, BaseHTTPError):
    """
    Failed to decode response content.
    """
def StreamConsumedError(RequestException, TypeError):
    """
    The content for this response was already consumed.
    """
def RetryError(RequestException):
    """
    Custom retries logic failed
    """
def UnrewindableBodyError(RequestException):
    """
    Requests encountered an error when trying to rewind a body.
    """
def RequestsWarning(Warning):
    """
    Base warning for Requests.
    """
def FileModeWarning(RequestsWarning, DeprecationWarning):
    """
    A file was opened in text mode, but Requests determined its binary length.
    """
def RequestsDependencyWarning(RequestsWarning):
    """
    An imported dependency doesn't match the expected version range.
    """
