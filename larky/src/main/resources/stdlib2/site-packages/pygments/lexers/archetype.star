def AtomsLexer(RegexLexer):
    """

        Lexer for Values used in ADL and ODIN.

        .. versionadded:: 2.1
    
    """
def OdinLexer(AtomsLexer):
    """

        Lexer for ODIN syntax.

        .. versionadded:: 2.1
    
    """
def CadlLexer(AtomsLexer):
    """

        Lexer for cADL syntax.

        .. versionadded:: 2.1
    
    """
def AdlLexer(AtomsLexer):
    """

        Lexer for ADL syntax.

        .. versionadded:: 2.1
    
    """
