def SMLLexer(RegexLexer):
    """

        For the Standard ML language.

        .. versionadded:: 1.5
    
    """
    def stringy(whatkind):
        """
        r'[^"\\]'
        """
    def long_id_callback(self, match):
        """
         Whitespace and comments are (almost) everywhere
        'whitespace'
        """
