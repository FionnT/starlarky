def PackagingMagics(Magics):
    """
    Magics related to packaging & installation
    """
    def pip(self, line):
        """
        Run the pip package manager within the current kernel.

                Usage:
                  %pip install [pkgs]
        
        """
    def conda(self, line):
        """
        Run the conda package manager within the current kernel.
        
                Usage:
                  %conda install [pkgs]
        
        """
